function findlowestNum (numbers){
    let minNum = numbers[0];
    for(let number of numbers){
        const value = number;
        if(value < minNum){
            minNum = value;
        }
    }
    return minNum;   
}
const numArray = [167, 190, 120, 165, 137];
const result = findlowestNum(numArray);
console.log(result);  
